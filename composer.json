{
    "name": "mikehins/pepperidge",
    "description": "A description for Pepperidge.",
    "type": "package",
    "license": "MIT",
    "keywords": [
        "laravel"
    ],
    "authors": [
        {
            "name": "Mike Hins",
            "email": "mike@hins.dev"
        }
    ],
    "require": {
        "illuminate/support": "^7.0|^8.0|^9.0"
